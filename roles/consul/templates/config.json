{
    "bootstrap_expect": 3,
    "client_addr": "0.0.0.0",
    "datacenter": "us-east-2",
    "data_dir": "/var/consul",
    "domain": "consul",
    "enable_script_checks": true,
    "dns_config": {
        "enable_truncate": true,
        "only_passing": true
    },
    "enable_syslog": true,
    "encrypt": "{{consul_key}}",
    "leave_on_terminate": true,
    "log_level": "INFO",
    "rejoin_after_leave": true,
    "server": true,
    "start_join": [
        "groups['master']",
        "groups['servers']"
    ],
    "ui": true
}

